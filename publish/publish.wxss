.contaioner{
  height: 100%;
  width: 100%;
}
.part1{
  position: absolute;
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: space-between;
  height: 8%;
  width: 95%;
  left: 2%;
  border-top:1px solid rgba(128, 128, 128, 0.438);
  border-bottom: 1px solid rgba(128, 128, 128, 0.438);
}

.text_all{
  margin-left: 3%;
}

.input_all{
  height: 75%;
  min-height: 75%; 
  width: 70%;
  border: 1px solid rgba(128, 128, 128, 0.438);
}

.part2{
  position: absolute;
  top: 10%;
  left: 2%;
  height: 18%;
  width: 95%;
  display: flex;
  flex-direction: column;
  align-items: left;  
  border-bottom: 1px solid rgba(128, 128, 128, 0.438);
}

.part2 text{
  margin-left: 3%;
}

.part2 textarea{
  height: 60%;
  width: 90%;
  margin-top: 1%;
  margin-left: 3%;
  border: 1px solid rgba(128, 128, 128, 0.438);
}

.upload_contaioner{
  display: flex;
  flex-direction: row;
  height: 17%;
  width: 95%;
  position: absolute;
  top: 30%;
  left: 2%;
  border-bottom: 1px solid rgba(128, 128, 128, 0.438);
}

.upload{
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  height: 90%;
  width: 25%;
  left: 2%;
  margin-right: 5%;
  border: 1px solid rgba(128, 128, 128, 0.438);
}

.text1{
  font-size: 25px;
  color: rgba(128, 128, 128, 0.438);
}

.text2{
  font-size: 15px;
  color: rgba(128, 128, 128, 0.438);
}

.details_contaioner{
  position: absolute;
  top: 47%;
  height: 40%;
  width: 95%;
  left: 2%;
}

.detalis_box{
  position: relative;
  height: 20%;
  width: 100%;
  border-bottom: 1px solid rgba(128, 128, 128, 0.438);
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: space-between;
}

.submit{
  position: absolute;
  text-align: center;
  left:40%; 
  bottom: 1%;
  height: 10%;
  width: 17%;
  font-size: 11px;
  color: none;
  border-radius: 50%;
  background-color: #fff;
  border: 1px solid rgba(128, 128, 128, 0.438);
  line-height: 100rpx;
}